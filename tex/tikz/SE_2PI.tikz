\begin{feynman}
    \vertex[] (in);
    \vertex[right=of in, label=center:1PI, blob, fill=n@terracotta] (blob1) {\contour{black}{}};
    \vertex[right=of blob1, label=center:1PI, blob, fill=n@terracotta] (blob2) {\contour{black}{}};
    \vertex[right=of blob2] (out);

    \diagram*{
    (in) -- [scalar] (blob1)
         -- [scalar] (blob2)
         -- [scalar] (out)
    };

\end{feynman}
